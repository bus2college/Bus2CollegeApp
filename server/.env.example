# Environment Configuration
NODE_ENV=development
PORT=3000

# Frontend URL (CORS)
FRONTEND_URL=http://localhost:5500

# Claude API Key (for AI suggestions)
CLAUDE_API_KEY=your-api-key-here

# College Board API (if available)
COLLEGEBOARD_API_KEY=your-api-key-here

# Common App API (if available)
COMMONAPP_API_KEY=your-api-key-here

# Scraping Configuration
ENABLE_WEB_SCRAPING=true
SCRAPING_DELAY_MS=1000

# Cache Configuration
CACHE_TTL_COLLEGES=86400
CACHE_TTL_DEADLINES=3600

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
